.my_page {
  width: 100%;
  height: 100%;
}
.UCenter-bg {
  /* background-image: url(https://image.weilanwl.com/color2.0/index.jpg); */
  background-image: var(--gradualBlue);
  background-size: cover;
  height: 500rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}
.cu-tag-size.capsule {
  padding: 20rpx;
}
map,
.mapBox {
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map,
.mapBox {
  width: 750rpx;
  height: 300rpx;
}

.capsule {
  width: 58rpx;
  height: 58rpx;
}
.mypage_container {
  width: 100%;
  height: calc(100% - 500rpx);
}
.mypage_container_scroll {
  width: 100%;
  height: 100%;
}
.van-sticky {
  box-shadow: 2rpx 10rpx 11rpx 0rpx rgba(0, 102, 204, 0.1);
  z-index: 10;
}
.van-tabs {
  width: 100%;
  height: 100%;
}
.van-tabs__line {
  background-color: var(--blue);
}
.van-tabs__content {
  height: calc(100% - 44px);
}
.van-tabs__track {
  height: 100%;
}
.van-tab__pane {
  height: 100%;
  color: #fff;
}
.nav-item-scroll {
  width: 100%;
  height: 100%;
}
.scroll-item-card {
  width: 100%;
}
.scroll-item-ceshi:nth-child(1) {
  margin-top: 20rpx;
}
.desc {
  color: #888;
}
