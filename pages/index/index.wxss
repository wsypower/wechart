.container {
	display: flex;
	flex-direction: column;
	align-items: center;
	width: 100%;
}
.header {
	position: fixed;
	background-color: #ffffff;
	height: auto;
	width: 100%;
	border-top: 1px solid #f5f5f5;
	border-bottom: 1px solid #f5f5f5;
	display: flex;
	flex-direction: column;
	box-shadow: 0 0 3px 0 #e3e3e3;
	z-index: 99;
}
.header_search {
	width: 100%;
	height: 100%;
}
.box {
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	border-radius: 50px;
	background-color: #f5f5f5;
	height: 68rpx;
	width: 700rpx;
	color: #999999;
}
.sub-container {
	width: 100%;
	height: 200rpx;
	background-color: royalblue;
	margin-top: 500rpx;
	color: #fff;
}
.body{
    width: 100%;
    height: 3100rpx;
    background-color: red;
}